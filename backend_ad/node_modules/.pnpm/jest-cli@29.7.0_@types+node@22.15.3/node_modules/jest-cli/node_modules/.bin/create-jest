#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/User/Desktop/website project/test 3.1/backend_ad/node_modules/.pnpm/create-jest@29.7.0_@types+node@22.15.3/node_modules/create-jest/bin/node_modules:/mnt/c/Users/User/Desktop/website project/test 3.1/backend_ad/node_modules/.pnpm/create-jest@29.7.0_@types+node@22.15.3/node_modules/create-jest/node_modules:/mnt/c/Users/User/Desktop/website project/test 3.1/backend_ad/node_modules/.pnpm/create-jest@29.7.0_@types+node@22.15.3/node_modules:/mnt/c/Users/User/Desktop/website project/test 3.1/backend_ad/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/User/Desktop/website project/test 3.1/backend_ad/node_modules/.pnpm/create-jest@29.7.0_@types+node@22.15.3/node_modules/create-jest/bin/node_modules:/mnt/c/Users/User/Desktop/website project/test 3.1/backend_ad/node_modules/.pnpm/create-jest@29.7.0_@types+node@22.15.3/node_modules/create-jest/node_modules:/mnt/c/Users/User/Desktop/website project/test 3.1/backend_ad/node_modules/.pnpm/create-jest@29.7.0_@types+node@22.15.3/node_modules:/mnt/c/Users/User/Desktop/website project/test 3.1/backend_ad/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../create-jest@29.7.0_@types+node@22.15.3/node_modules/create-jest/bin/create-jest.js" "$@"
else
  exec node  "$basedir/../../../../../create-jest@29.7.0_@types+node@22.15.3/node_modules/create-jest/bin/create-jest.js" "$@"
fi
